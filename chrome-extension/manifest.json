{
    "manifest_version": 2,
    "name": "SPHINX-WEB",
    "version": "1.0.0",

    "description": "A toy demo for sphinx password storage manager.",

    "background":
    {
        "persistent": true,
        "page": "background.html"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "nativeMessaging",
        "webRequest",
        "storage",
        "notifications"
    ],
    "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://www.googletagmanager.com/ https://www.googleapis.com; object-src 'self'"
}